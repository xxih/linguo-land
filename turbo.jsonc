{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**", "!.next/cache/**", "app/docs/build/**"],
    },
    "build:dev": {
      "dependsOn": ["^build:dev"],
      "outputs": ["dist/**", ".next/**", "!.next/cache/**", "app/docs/build/**"],
    },
    "dev": {
      "cache": false,
      "persistent": true,
    },
  },
}
