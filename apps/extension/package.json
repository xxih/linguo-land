{
  "name": "extension",
  "private": true,
  "version": "0.2.9",
  "type": "module",
  "scripts": {
    "dev:ui": "vite --config vite.config.content-ui.dev.ts",
    "dev:options": "vite --config vite.config.options.dev.ts --mode development",
    "build:content-ui:iife": "vite build --config vite.config.content-ui.iife.ts",
    "build:content-ui:css": "vite build --config vite.config.content-ui.css.ts",
    "build:content:iife": "vite build --config vite.config.content.iife.ts",
    "build:background:iife": "vite build --config vite.config.iife.background.ts",
    "build:popup-options": "vite build --config vite.config.ts",
    "build": "npm run build:content-ui:iife && npm run build:content-ui:css && npm run build:popup-options && npm run build:background:iife && npm run build:content:iife",
    "build:dev:content-ui:iife": "vite build --config vite.config.content-ui.iife.ts --mode development",
    "build:dev:content-ui:css": "vite build --config vite.config.content-ui.css.ts --mode development",
    "build:dev:content:iife": "vite build --config vite.config.content.iife.ts --mode development",
    "build:dev:background:iife": "vite build --config vite.config.iife.background.ts --mode development",
    "build:dev:popup-options": "vite build --config vite.config.ts --mode development",
    "build:dev": "npm run build:dev:content-ui:iife && npm run build:dev:content-ui:css && npm run build:dev:popup-options && npm run build:dev:background:iife && npm run build:dev:content:iife",
    "version:bump": "bumpp package.json public/manifest.json --patch",
    "build:release": "npm run version:bump && npm run build",
    "build:backup": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:run": "vitest run"
  },
  "dependencies": {
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
